{
    "vocab_size": 262144,
    "hidden_size": 2048,
    "num_hidden_layers": 24,
    "num_attention_heads": 16,
    "num_key_value_heads": 8,
    "intermediate_size": 8192,
    "sequence_length": 2048,
    "activation_function": "swiGLU",
    "max_position_embeddings": 2048,
    
    "batch_size": 64,
    "micro_batch_size": 2,
    "inner_steps": 12,
    "outer_learning_rate": 0.05,
    "blocks_per_window": 15,
    "time_window_delta_seconds": 150,
    
    "validator_sample_micro_bs": 16,
    "eval_lr_factor": 0.5,
    
    "offload_optimizer_states": false,
    "fsdp_reshard_after_forward": "false",
    
    "checkpoint_init_version": null,
    "checkpoint_init_window": null,
    
    "optimizer": {
        "type": "adamw",
        "adamw": {
            "learning_rate": 2e-4,
            "scheduler": {
                "warmup_steps": 300,
                "t_max": 1000,
                "eta_min_factor": 0.1
            }
        }
    },
    
    "torchtitan": {
        "tie_embeddings": false,
        "enable_compiled_autograd": true,
        "enable_async_tensor_parallel": false
    }
}
